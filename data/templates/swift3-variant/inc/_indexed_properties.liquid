{%- if entity.has_indexed_attributes == true %}
	// Specify properties to index
	override static func indexedProperties() -> [String] {
		return [
			{%- capture enum_indexed_properties -%}
				{%- for attribute in entity.attributes %}
					{%- if attribute.is_indexed == true %}
						"{{attribute.name}}",
					{%- endif %}
				{%- endfor %}
			{%- endcapture -%}

    	{%- assign enum_indexed_properties_size = enum_indexed_properties | size | minus: 1 -%}
    	{{ enum_indexed_properties | truncate: enum_indexed_properties_size, '' }}]
	}
{%- endif -%}