
{% if entity.attributes.size > 0 %}
  enum Attributes: String {
    {%- for attribute in entity.attributes %}
        {%- if attribute.realm_ignored or attribute.realm_read_only.size == 0 %}
            {%- if attribute.comment.size > 0 %}
    /** {{ attribute.comment }} **/
            {%- endif %}
    case {{ attribute.name }} = {% if attribute.json_key_path.size > 0 -%}
            "{{ attribute.json_key_path | escape_quotes }}"
          {%- else -%}
            "{{ attribute.name | escape_quotes }}"
          {%- endif %}
        {%- endif %}
    {%- endfor %}
  }
{% endif -%}