{%- if relationship.type == "to_many" %}
	let {{ relationship.name | truncate: {{ relationship.name.size | minus:1 }}, '' }}s = LinkingObjects(fromType: {{ relationship.inverse_type }}.self, property: "{{ relationship.inverse_name }}")
		{%- else %}
